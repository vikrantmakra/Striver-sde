void sort012(int *arr, int n)
{
   //   Write your code here
    int l=0,m=0,h=n-1;
    while(m<=h)
    {
        if(arr[m]==0)
        {
            swap(arr[l],arr[m]);
            l++;
            m++;
            
        }
        else if(arr[m]==1)
        {
            m++;
        }
        else
        {
            swap(arr[m],arr[h]);
            h--;
        }
    }
    
}
